.navbar.navbar-fixed-top
  .navbar-inner
    .container
      %a.btn.btn-navbar{:'data-target' => '.nav-collapse', :'data-toggle' => 'collapse'}
        %span.icon-bar
        %span.icon-bar
        %span.icon-bar

      .container.nav-collapse
        %ul.nav#main_nav
          - @level1.each do |obj|
            %li{id: "main_nav_page_#{obj.id}"}
              = link_to(cms_path(obj)) do
                = display_title(obj)

        - unless Filters::EnvironmentDetection.live_environment?
          = render_cell(:workspace_toggle, :show)